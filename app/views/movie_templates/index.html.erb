<h1>List of all movies</h1>

<table border = "1">
<% @list_of_movies.each do |a_thing| %>
<% @list_of_directors.each do |a_thing2| %>
  <tr>
    <td><%= a_thing.id %></td>
    <td>
      <img src="http://robohash.org/<%= a_thing.title %>"
    </td>
    <td><%= a_thing.title %></td>
    <td><%= a_thing.year %></td>
    <td><%= a_thing2.name %></td>
    <td>
      <a href="/movies/<%= a_thing.id %>">
      Show details
      </a>
    </td>
  </tr>
<% end %>
<% end %>
</table>
